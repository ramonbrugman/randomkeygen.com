{
  "name": "randomkeygen.com",
  "version": "1.0.0",
  "description": "Random Key Generator",
  "private": true,
  "scripts": {
    "test": "npm run lint",
    "build": "npm run clean && NODE_ENV=production webpack --mode=production",
    "dev": "NODE_ENV=development webpack serve --port 8107",
